{
  "name": "yellowjacket",
  "version": "0.1.0",
  "description": "Scalable task runner",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhoriuchi/yellowjacket.git"
  },
  "scripts": {
    "build:copybrc": "cp build/.build.babelrc src/.babelrc",
    "build:rmbrc": "rm -f src/.babelrc",
    "example": "node example/index.js --harmony",
    "build": "npm run build:copybrc && rollup -c build/rollup.config.js && npm run build:rmbrc",
    "test": "npm run build && mocha test/index.js"
  },
  "author": "Branden Horiuchi <bhoriuchi@gmail.com>",
  "dependencies": {
    "bunyan": "^1.8.1",
    "chalk": "^1.1.3",
    "graphql-factory": "^0.4.0",
    "graphql-factory-types": "^0.1.0",
    "lodash": "^4.13.1",
    "node-getopt": "^0.2.3",
    "socket.io": "^1.4.0",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "6.13.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-runtime": "^6.9.0",
    "babel-register": "^6.11.0",
    "graphql": "^0.6.0",
    "mocha": "^3.0.0",
    "rethinkdbdash": "^2.3.6",
    "rewire": "^2.5.1",
    "rollup": "^0.34.0",
    "rollup-plugin-babel": "^2.6.0",
    "babel-preset-es2015-rollup": "^1.1.0"
  },
  "bugs": {
    "url": "https://github.com/bhoriuchi/yellowjacket/issues"
  },
  "homepage": "https://github.com/bhoriuchi/yellowjacket#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "task",
    "runner"
  ]
}
